
<screen>
<preview
file="app/watchface1/wf1preview.bin">
</preview>
    <img
            x="0"
            y="0"
            opacity="255"
            file="app/watchface1/Slice1.bin"
            blendMode="imgBypassMode"
            >watchface
    </img>
    <img
            x="221"
            y="146"
            opacity="255"
            file="app/watchface1/Union2.bin"
            blendMode="imgSrcOverMode"
            >hour_hand
            <onTime
                type="hour"
                to="hour_hand">
            </onTime>
    </img>
    <img
            x="222"
            y="66"
            opacity="255"
            file="app/watchface1/Union.bin"
            blendMode="imgSrcOverMode"
            >minute_hand
            <onTime
                type="minute"
                to="minute_hand">
            </onTime>
    </img>
    <img
            x="228"
            y="100"
            opacity="255"
            file="app/watchface1/Vector7.bin"
            blendMode="imgSrcOverMode"
            >second_hand
            <onTime
                type="second"
                to="second_hand">
            </onTime>
    </img>
    <img
            x="226"
            y="222"
            opacity="255"
            file="app/watchface1/Ellipse5.bin"
            blendMode="imgSrcOverMode"
            >mask
            
    </img>
    <icon
            picture="app/watchface1/Group21.bin"
            highlightPicture="app/watchface1/Group21hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="57"
            y="57">btn_music
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
    </icon>
    <icon
            picture="app/watchface1/Group23.bin"
            highlightPicture="app/watchface1/Group23hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="57"
            y="342">btn_light
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
    </icon>
    <arc
        centerX="85"
        centerY="369"
        radius="27"
        strokeWidth="4"
        strokeColor="yellow"
        startAngle="-1.6"
        endAngle="3.14"
        clockwise="CW"
        cap="round">arc1
        <onPeripheral 
            type="activity" 
            to="arc1" 
            id="exercise">
        </onPeripheral>
    </arc>
    <icon
            picture="app/watchface1/Group22.bin"
            highlightPicture="app/watchface1/Group22hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="342"
            y="342">btn_sport
    </icon>
    <icon
            picture="app/watchface1/Ellipse19.bin"
            highlightPicture="app/watchface1/Ellipse19hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="342"
            y="57">btn_step
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
            <textbox
            x="0"
            y="15"
            color="white"
            w="55"
            h="49"
            text="97"
            fontSize="24"
            font="app/watchface1/MichromaRegularSize24Bits4Font.bin"
            mode="center">txt_brightness
            <onPeripheral 
                type="Weather" 
                to="txt_brightness" 
                id="current">
            </onPeripheral>

            </textbox>
    </icon>
    <textbox
            x="0"
            y="130"
            color="white"
            w="454"
            h="49"
            text="SATURDAY"
            fontSize="24"
            font="app/watchface1/MichromaRegularSize24Bits4Font.bin"
            mode="center">txt_week
            <onTime
                type="MONDAY"
                to="txt_week"
                >
            </onTime>
    </textbox>
    <textbox
            x="0"
            y="160"
            color="white"
            w="454"
            h="49"
            text="12/MARCH"
            fontSize="16"
            font="app/watchface1/MichromaRegularSize16Bits4Font.bin"
            mode="center">txt_date
            <onTime
                type="1/JANUARY"
                to="txt_date"
                >
            </onTime>
    </textbox> 

</screen>
