
<screen>
<preview
file="app/watchface1/wf1preview.bin">
</preview>
    <img
            x="0"
            y="0"
            opacity="255"
            file="app/watchface1/w1Slice1.bin"
            blendMode="imgBypassMode"
            >watchface
    </img>
    <img
            x="199"
            y="170"
            opacity="255"
            file="app/watchface1/w1Union2.bin"
            blendMode="imgSrcOverMode"
            >hour_hand
            <onTime
                type="hour"
                to="hour_hand">
            </onTime>
    </img>
    <img
            x="200"
            y="90"
            opacity="255"
            file="app/watchface1/w1Union.bin"
            blendMode="imgSrcOverMode"
            >minute_hand
            <onTime
                type="minute"
                to="minute_hand">
            </onTime>
    </img>
    <img
            x="206"
            y="124"
            opacity="255"
            file="app/watchface1/w1Vector7.bin"
            blendMode="imgSrcOverMode"
            >second_hand
            <onTime
                type="second"
                to="second_hand">
            </onTime>
    </img>
    <img
            x="204"
            y="246"
            opacity="255"
            file="app/watchface1/w1Ellipse5.bin"
            blendMode="imgSrcOverMode"
            >mask
            
    </img>
    <icon
            picture="app/watchface1/w1Group21.bin"
            highlightPicture="app/watchface1/w1Group21hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="35"
            y="57">btn_music
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
    </icon>
    <icon
            picture="app/watchface1/w1Group23.bin"
            highlightPicture="app/watchface1/w1Group23hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="35"
            y="366">btn_light
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
    </icon>
    <arc
        centerX="63"
        centerY="393"
        radius="27"
        strokeWidth="4"
        strokeColor="yellow"
        startAngle="-1.6"
        endAngle="3.14"
        clockwise="CW"
        cap="round">arc1
        <onPeripheral 
            type="activity" 
            to="arc1" 
            id="exercise">
        </onPeripheral>
    </arc>
    <icon
            picture="app/watchface1/w1Group22.bin"
            highlightPicture="app/watchface1/w1Group22hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="320"
            y="366">btn_sport
    </icon>
    <icon
            picture="app/watchface1/w1Ellipse19.bin"
            highlightPicture="app/watchface1/w1Ellipse19hl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="320"
            y="57">btn_step
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
            <textbox
            x="0"
            y="15"
            color="white"
            w="55"
            h="49"
            text="97"
            fontSize="24"
            font="app/watchface1/w1MichromaRegularSize24Bits4Font.bin"
            mode="center">txt_brightness
            <onPeripheral 
                type="Weather" 
                to="txt_brightness" 
                id="current">
            </onPeripheral>

            </textbox>
    </icon>
    <textbox
            x="0"
            y="130"
            color="white"
            w="410"
            h="49"
            text="SATURDAY"
            fontSize="24"
            font="app/watchface1/w1MichromaRegularSize24Bits4Font.bin"
            mode="center">txt_week
            <onTime
                type="MONDAY"
                to="txt_week"
                >
            </onTime>
    </textbox>
    <textbox
            x="0"
            y="160"
            color="white"
            w="410"
            h="49"
            text="12/MARCH"
            fontSize="16"
            font="app/watchface1/w1MichromaRegularSize16Bits4Font.bin"
            mode="center">txt_date
            <onTime
                type="1/JANUARY"
                to="txt_date"
                >
            </onTime>
    </textbox> 

</screen>
