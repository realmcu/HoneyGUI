    <chart
        w="300"
        h="40"
        x="15"
        y="30"
        color="#F44101"
        items="100,200,100,0,110,50,150,100"
        max="200"
        min="0"  
        >chart1
        <onPeripheral 
                type="heart_rate" 
                to="chart1" 
                id="array">
            </onPeripheral>
           
</chart>  
            </textbox>
            <img
            x="222"
            y="222"
            opacity="255"
