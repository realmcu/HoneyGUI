
<screen>
<preview
file="app/watchface2/wf2preview.bin">
</preview>
    <img
            x="0"
            y="0"
            opacity="255"
            file="app/watchface2/w2w.bin"
            blendMode="imgBypassMode"
            >watchface
    </img>
    <textbox
            x="60"
            y="230"
            color="white"
            w="55"
            h="49"
            text="90"
            fontSize="16"
            font="app/watchface2/MichromaRegularSize16Bits4Font.bin"
            mode="center">txt_HR
           <onPeripheral 
                type="heart_rate" 
                to="txt_HR" 
                id="current">
            </onPeripheral>
 <textbox
            x="45"
            y="0"
            color="white"
