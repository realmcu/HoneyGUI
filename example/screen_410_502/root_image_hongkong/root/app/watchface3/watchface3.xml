
<screen>
<preview
file="app/watchface3/wf3preview.bin">
</preview>
    <img
            x="0"
            y="0"
            opacity="255"
            file="app/watchface3/w3w.bin"
            blendMode="imgBypassMode"
            >watchface
    </img>
        <img
            x="200"
            y="246"
            opacity="255"
            file="app/watchface3/w3m1.bin"
            blendMode="imgSrcOverMode"
            >mask
            
    </img>
    <img
            x="199"
            y="165"
            opacity="255"
            file="app/watchface3/w3hh.bin"
            blendMode="imgSrcOverMode"
            >hour_hand
            <onTime
                type="hour"
                to="hour_hand">
            </onTime>
    </img>
    <img
            x="200"
            y="94"
            opacity="255"
            file="app/watchface3/w3mh.bin"
            blendMode="imgSrcOverMode"
            >minute_hand
            <onTime
                type="minute"
                to="minute_hand">
            </onTime>
    </img>
    <img
            x="204"
            y="124"
            opacity="255"
            file="app/watchface3/w3sh.bin"
            blendMode="imgSrcOverMode"
            >second_hand
            <onTime
                type="second"
                to="second_hand">
            </onTime>
    </img>
        <img
            x="202"
            y="248"
            opacity="255"
            file="app/watchface3/w3m2.bin"
            blendMode="imgSrcOverMode"
            >mask2
            
    </img>
    <icon
            picture="app/watchface3/w3music.bin"
            highlightPicture="app/watchface3/w3musichl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="64"
            h="64"
            x="35"
            y="57">btn_music
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_MUSIC">
            </onClick>
    </icon>
    <icon
            picture="app/watchface3/w3activity.bin"
            highlightPicture="app/watchface3/w3activityhl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="35"
            y="390">btn_activity
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_ACTIVITY">
            </onClick>
    </icon>

    <switch
            picture="app/watchface3/w3light.bin"
            highlightPicture="app/watchface3/w3switchhl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="55"
            h="55"
            x="320"
            y="390">btn_light
    </switch>
    <icon
            picture="app/watchface3/w3weather.bin"
            highlightPicture="app/watchface3/w3weatherhl.bin"
            blendMode="imgSrcOverMode"
            opacity="255"
            pictureX="0"
            pictureY="0"
            rotationAngle="0"
            w="64"
            h="64"
            x="320"
            y="57">btn_step
            <onClick
                    to="C-APP"
                    type="jump" 
                    id="APP_WEATHER">
            </onClick>
            <textbox
            x="0"
            y="19"
            color="white"
            w="55"
            h="49"
            text="97"
            fontSize="24"
            font="app/watchface3/w3earthorbiter_size24_bits4_font.bin"
            mode="center">txt_weather
            <onPeripheral 
                type="Weather" 
                to="txt_weather" 
                id="current">
            </onPeripheral>

            </textbox>
                        <textbox
            x="20"
            y="19"
            color="white"
            w="55"
            h="49"
            text=" C"
            fontSize="24"
            font="app/watchface3/w3earthorbiter_size24_bits4_font.bin"
            mode="center">txt_weather
     

            </textbox>
    </icon>
    <textbox
            x="0"
            y="260"
            color="white"
            w="410"
            h="49"
            text="Mon 1"
            fontSize="24"
            font="app/watchface3/w3earthorbiter_size24_bits4_font.bin"
            mode="center">txt_date1
            <onTime
                type="Mon 1"
                to="txt_date1"
                >
            </onTime>
    </textbox>
    <textbox
            x="0"
            y="300"
            color="white"
            w="410"
            h="49"
            text="00:00"
            fontSize="80"
            font="app/watchface3/w3earthorbiter_size80_bits4_font.bin"
            mode="center">txt_date2
            <onTime
                type="00:00"
                to="txt_date2"
                >
            </onTime>
    </textbox> 
 <img
            x="199"
            y="426"
            opacity="255"
            file="app/watchface3/w3hr.bin"
            blendMode="imgSrcOverMode"
            >img_hr
            
    </img>
    <textbox
            x="0"
            y="436"
            color="#F44101"
            w="410"
            h="49"
            text="97"
            fontSize="24"
            font="app/watchface3/w3earthorbiter_size24_bits4_font.bin"
            mode="center">txt_hr
            <onPeripheral 
                type="heart_rate" 
                to="txt_hr" 
                id="current">
            </onPeripheral>

            </textbox>
</screen>
