
from building import *

cwd = GetCurrentDir()
src = Glob('*.c')

CPPPATH = [cwd]

CPPDEFINES = []
if GetDepend('CONFIG_REALTEK_BUILD_LETTER_SHELL'):
    CPPDEFINES += ['ENABLE_RTK_GUI_CONSOLE']

objs = DefineGroup('realgui/3rd', src, depend = ['CONFIG_REALTEK_BUILD_LETTER_SHELL'], CPPPATH = CPPPATH, CPPDEFINES = CPPDEFINES)

Return('objs')
