# HoneyGUI building script

from building import *

cwd = GetCurrentDir()
src = [cwd + '/port.c']
src += [cwd + '/jerry_util.c']
src += [cwd + '/jerry_console.c']
src += [cwd + '/jerry_utf8.c']
CPPPATH = [cwd]

group = DefineGroup('JerryScriptPort', src, depend = ['CONFIG_REALTEK_HONEYGUI'], CPPPATH = CPPPATH)

Return('group')
