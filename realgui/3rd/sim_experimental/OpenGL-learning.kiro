                    "Text": "/*\n * OpenGL 2.1 + GLFW Demo (Programmable Pipeline)\n * - OBJ loader (triangles only)\n * - VBO + attribute arrays\n * - GLSL 1.20 vertex/fragment shaders\n * - Uniform matrices (model/view/projection) and normal matrix\n * - Reads back pixels to push to LCD via fb_sdl.h\n */\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdint.h>\n#include <string.h>\n#include <math.h>\n#include <pthread.h>\n#include <unistd.h>\n#define GL_GLEXT_PROTOTYPES\n#include <GLFW/glfw3.h>\n#include <GL/glu.h>\n#include \"fb_sdl.h\"\n\n#define MAX_VERTICES 1000\n#define MAX_FACES 2000\n\nstatic float vertices[MAX_VERTICES][3];\nstatic float face_normals[MAX_FACES][3];\nstatic int faces[MAX_FACES][3];\nstatic int vertex_count = 0;\nstatic int face_count = 0;\n\n// GLSL 1.20 shaders (attributes + varyings)\nstatic const char *vs_src =\n    \"#version 120\\n\"\n    \"attribute vec3 aPos;\\n\"\n    \"attribute vec3 aNormal;\\n\"\n    \"uniform mat4 uModel;\\n\"\n    \"uniform mat4 uView;\\n\"\n    \"uniform mat4 uProj;\\n\"\n    \"uniform mat3 uNormalMat;\\n\"\n    \"varying vec3 vWorldPos;\\n\"\n    \"varying vec3 vNormal;\\n\"\n    \"void main(){\\n\"\n    \"  vec4 wp = uModel * vec4(aPos, 1.0);\\n\"\n    \"  vWorldPos = wp.xyz;\\n\"\n    \"  vNormal = normalize(uNormalMat * aNormal);\\n\"\n    \"  gl_Position = uProj * uView * wp;\\n\"\n    \"}\\n\";\n\nstatic const char *fs_src =\n    \"#version 120\\n\"\n    \"varying vec3 vWorldPos;\\n\"\n    \"varying vec3 vNormal;\\n\"\n    \"uniform vec3 uCamPos;\\n\"\n    \"uniform vec3 uLightDir;\\n\"\n    \"uniform vec3 uLightColor;\\n\"\n    \"uniform vec3 uBaseColor;\\n\"\n    \"void main(){\\n\"\n    \"  vec3 N = normalize(vNormal);\\n\"\n    \"  vec3 L = normalize(-uLightDir);\\n\"\n    \"  vec3 V = normalize(uCamPos - vWorldPos);\\n\"\n    \"  vec3 H = normalize(L + V);\\n\"\n    \"  float diff = max(dot(N,L), 0.0);\\n\"\n    \"  float spec = pow(max(dot(N,H), 0.0), 16.0);\\n\"\n    \"  vec3 color = uBaseColor * diff + uLightColor * spec;\\n\"\n    \"  gl_FragColor = vec4(color, 1.0);\\n\"\n    \"}\\n\";\n\nstatic GLuint compile(GLenum type, const char *src)\n{\n    GLuint s = glCreateShader(type);\n    glShaderSource(s, 1, &src, NULL);\n    glCompileShader(s);\n    GLint ok = 0; glGetShaderiv(s, GL_COMPILE_STATUS, &ok);\n    if (!ok) {\n        char log[1024]; glGetShaderInfoLog(s, 1024, NULL, log);\n        fprintf(stderr, \"shader compile error: %s\\n\", log);\n    }\n    return s;\n}\n\nstatic GLuint link_program(GLuint vs, GLuint fs)\n{\n    GLuint p = glCreateProgram();\n    // Bind attribute locations before linking (GLSL 1.20 has no layout qualifiers)\n    glAttachShader(p, vs);\n    glAttachShader(p, fs);\n    glBindAttribLocation(p, 0, \"aPos\");\n    glBindAttribLocation(p, 1, \"aNormal\");\n    glLinkProgram(p);\n    GLint ok = 0; glGetProgramiv(p, GL_LINK_STATUS, &ok);\n    if (!ok) {\n        char log[1024]; glGetProgramInfoLog(p, 1024, NULL, log);\n        fprintf(stderr, \"program link error: %s\\n\", log);\n    }\n    return p;\n}\n\nstatic void load_obj(const char *filename)\n{\n    FILE *f = fopen(filename, \"r\");\n    if (!f) { printf(\"Cannot open %s\\n\", filename); return; }"
