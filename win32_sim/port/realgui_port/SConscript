# building script for HoneyGUI

from building import *

cwd = GetCurrentDir()
src = Split("""
""")

src = Glob('*.c')
CPPPATH = [cwd]
CPPDEFINES = []

if GetDepend('CONFIG_REALTEK_BUILD_MONKEY_TEST'):
    CPPDEFINES += ['ENABLE_MONKEY_TEST']

group = DefineGroup('rtk_gui', src, depend = ['CONFIG_REALTEK_HONEYGUI'], CPPPATH = CPPPATH, CPPDEFINES = CPPDEFINES)

Return('group')
