			<tab idx="5">t2
				<icon x="100" y="10" w="100" h="100" 
					picture="app/test/resource/icMenuAlbum.bin" 
					highlightPicture="app/test/resource/icMenuNotification.bin" 
					mode="normal" text="normal" textX="0" textY="70">iconNormal
				</icon>
				<icon x="100" y="120" w="100" h="100" 
					picture="app/test/resource/icMenuAlbum.bin" 
					highlightPicture="app/test/resource/icMenuNotification.bin"  
					mode="fade" text="fade" textX="0" textY="70">iconFade
				</icon>
				<icon x="100" y="230" w="100" h="100" 
					picture="app/test/resource/icMenuAlbum.bin" 
					highlightPicture="app/test/resource/icMenuNotification.bin"  
					mode="scale" text="scale" textX="0" textY="70">iconScale
				</icon>
				<icon x="100" y="340" w="100" h="100" 
					picture="app/test/resource/icMenuAlbum.bin" 
					highlightPicture="app/test/resource/icMenuNotification.bin"  
					mode="fade&scale" text="scale&fade" textX="0" textY="70">iconfade&scale
				</icon>

