<?xml version="1.0" encoding="UTF-8"?>
<app iconPicture="app/test/resource/icMenuNotification.bin">test
	<script file="app/test/test.js"></script>
	<screen>app_test
		<tabview >tv1
			<tab >t1
				<textbox text="图片img， 缩放和旋转" ></textbox>
				<img file="app/test/resource/icMenuNotification.bin" x="100" y="100">img1 </img>
